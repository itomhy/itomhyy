{
    "name": "𓆩ᴅs𓆪 Mᴜsɪ́ᴄ♪ ",
    "description": "Mükəmməl mahnı və video userbotu🎵🎥.",
    "logo": "https://telegra.ph/file/81a9e1d0da4d570012592.jpg",
    "keywords": [
        "Telegram",
        "MusicPlayer",
        "VideoPlayer"
    ],
    "repository": "https://github.com/Yushkabotss/Userbot2",
    "env": {
        "SESSION": {
            "description": "String Session yazın (@TG_StringGenBot -dan əldə edin) ",
            "required": true
        },
        "API_ID": {
            "description": "API ID yazın (@ApiInfobot -dan əldə edin)",
            "required": true
        },
        "PREFiKS": {
            "description": "Əmrlər üçün prefiks seçin (!,/)",
            "required": true,
            "value": "/"
        },
        "API_HASH": {
            "description": "API HASH yazın (@ApiInfobot -dan əldə edin)",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Telegram Hesabınızın ID-sini yazın (@MissRose_bot -a /id yazaraq əldə edin)",
            "required": true
        }
    },
    "addons": [],
    "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "container"
}
